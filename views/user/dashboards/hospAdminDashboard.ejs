    @import url('https://fonts.googleapis.com/css2?family=Roboto+Condensed:wght@400;700&display=swap');
    @import url('https://fonts.googleapis.com/css2?family=Oxygen&display=swap');
    
    #username{
        margin-top: 5px;
        font-weight: 300;
    }

    #name{
        margin: 0.8rem 0 0.5rem;
        font-family: 'Oxygen', sans-serif;
        font-weight: 700;
        color: rgb(2, 24, 10) ;
        display: block;
    }

    #createProfile{
        margin: 3rem 0 0.5rem;
        font-family: 'Roboto Condensed', sans-serif;
        font-weight: 700;
        text-decoration: none;
        color: rgb(10, 139, 60) ;
        display: block;
    }
    #updateProfile{
        margin: 1rem 0 0.8rem;
        font-family: 'Roboto Condensed', sans-serif;
        font-weight: 700;
        text-decoration: none;
        color: rgb(23, 124, 131) ;
        display: block;
    }
    #mainPageRedirect{
        margin: 1rem 0;
        font-family: 'Roboto Condensed', sans-serif;
        font-weight: 700;
        text-decoration: none;
        color: rgb(179, 7, 7) ;
        display: block;
    }

    .underline-from-center {
        -webkit-transform: translateZ(0);
        transform: translateZ(0);
        -webkit-backface-visibility: hidden;
        backface-visibility: hidden;
        -moz-osx-font-smoothing: grayscale;
        position: relative;
        overflow: hidden;
    }
    #mainPageRedirect.underline-from-center:before {
        content: "";
        position: absolute;
        z-index: -1;
        left: 50%;
        right: 50%;
        bottom: 0;
        background: rgb(179, 7, 7) ;
        height: 2px;
        -webkit-transition-property: left right;
        transition-property: left right;
        -webkit-transition-duration: 0.3s;
        transition-duration: 0.3s;
        -webkit-transition-timing-function: ease-out;
        transition-timing-function: ease-out;
    }

    #updateProfile.underline-from-center:before {
        content: "";
        position: absolute;
        z-index: -1;
        left: 50%;
        right: 50%;
        bottom: 0;
        background: rgb(23, 124, 131) ;
        height: 2px;
        -webkit-transition-property: left right;
        transition-property: left right;
        -webkit-transition-duration: 0.3s;
        transition-duration: 0.3s;
        -webkit-transition-timing-function: ease-out;
        transition-timing-function: ease-out;
    }
    #createProfile.underline-from-center:before {
        content: "";
        position: absolute;
        z-index: -1;
        left: 50%;
        right: 50%;
        bottom: 0;
        background: rgb(10, 139, 60) ;
        height: 2px;
        -webkit-transition-property: left right;
        transition-property: left right;
        -webkit-transition-duration: 0.3s;
        transition-duration: 0.3s;
        -webkit-transition-timing-function: ease-out;
        transition-timing-function: ease-out;
    }

    #mainPageRedirect.underline-from-center:hover:before {
        left: 1rem;
        right: 1rem;
    }
    #updateProfile.underline-from-center:hover:before {
        left: 20px;
        right: 20px;
    }
    #createProfile.underline-from-center:hover:before {
        left: 0;
        right: 0;
    }
</style>

  <body id="body-pd">
    <%-include("../../partials/altNavbar");-%>
    <%-include("../../partials/altAdminSideNavbar");-%>

      
