    <div class="row" style="margin: 8rem 4rem;">
        <div class="col-md-10">
            <% if(foundHosp.length > 0){ %>
                <h5><u><b>About your institutions</b></u></h5>
                <div style="line-height: 1.9rem;">
                    <p><strong>Hospital Name:</strong> <%=foundHosp[0].name%> </p>
                    <p><strong>Hospital Address:</strong> <%= foundHosp[0].address.street + ", " + foundHosp[0].address.city + ", " + foundHosp[0].address.state + ", " + foundHosp[0].address.zip %> </p>
                    <div class="row">
                        <div class="col-md-5">
                            <p><strong>Email: </strong><%=foundHosp[0].contact.email%></p>
                        </div>
                        <div class="col-md-5">
                            <p><strong>Phone Number: </strong><%=foundHosp[0].contact.phone%></p>
                        </div>
                    </div>
                </div>
            <% } else { %>
                <p><strong>No Hospital Registered to Current user!</strong></p>
            <% } %>

            <% if(foundAdmin.length > 0){ %>
            <h5 style="margin-top: 1rem"><u><strong>Administrator Information: </strong></u></h5>
            <div style="line-height: 1.9;">
                <p><strong>Admninstrator Name: </strong> <%= currentUser.firstName + " " + currentUser.lastName%></p>
                <div class="row">
                    <div class="col-md-5">
                        <p><strong>Administrator Email: </strong> <%= foundAdmin[0].adminContact.phone%></p>
                    </div>
                    <div class="col-md-5">
                        <p><strong>Administrator Contact No.: </strong> <%= foundAdmin[0].adminContact.phone%></p>
                    </div>
                </div>
            </div>  
            <% } else { %>
                <p><strong>No Admin is Registered   !</strong></p>
            <% } %>
            <h5 style="margin-top: 1rem"><u><Strong>Other Details: </Strong></u></h5>
            <% if(foundHosp.length > 0){ %>
            <p><strong>Hospital Token No: </strong><%= foundHosp[0]._id%></p>
            <% } %>
        </div>
        <div class="col-md-2"  data-aos="fade-left" style="border-radius: 25px;text-align: center;justify-content:center;box-shadow: 0 0 20px 0px rgb(0 0 0 / 20%);">
            <img src="https://cdn.iconscout.com/icon/premium/png-512-thumb/avatar-182-132174.png" class="img-fluid rounded-circle border" alt="https://png.pngtree.com/png-vector/20190223/ourmid/pngtree-vector-avatar-icon-png-image_695765.jpg" style="margin-top: 2rem; width:60%; box-shadow: 6px 5px 18px 0px rgba(0,0,0,0.33);" >
            <h5 id="username"><%=currentUser.username%></h5>
            <h5 id="name"><%= currentUser.firstName + " " + currentUser.lastName%></h5>
            <span><em>Joined <%= moment(currentUser.joinedAt).fromNow()%></em></span>
        </div>
    </div>

